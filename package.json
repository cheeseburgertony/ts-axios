{
  "name": "tony-axios",
  "version": "1.0.1",
  "description": "",
  "keywords": [],
  "main": "dist/axios.umd.js",
  "module": "dist/axios.es5.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "cheeseburgertony <1821955020@qq.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/cheeseburgertony/ts-axios"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "dev": "node examples/server.js",
    "lint": "eslint --fix --quiet src/**/*.ts __tests__/**/*.ts",
    "prepare": "husky",
    "lint-staged": "lint-staged",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.js --bundleConfigAsCjs && typedoc --out docs",
    "start": "rollup -c rollup.config.js -w",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "precommit": "lint-staged",
    "travis-deploy-once": "travis-deploy-once",
    "prepub": "npm run test:prod && npm run build",
    "pub": "sh release.sh"
  },
  "lint-staged": {
    "{src,__tests__}/**/*.ts": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "jsdom",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/setup/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/setup/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}",
      "src/**/*.{js,ts}"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/__tests__/setup/boot.ts"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@eslint/js": "^9.21.0",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@types/jasmine-ajax": "^3.3.5",
    "@types/jest": "^29.5.0",
    "@types/lodash.camelcase": "^4.3.7",
    "@types/node": "^20.0.0",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.14.0",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "atob": "^2.1.2",
    "colors": "^1.3.2",
    "commitizen": "^3.0.0",
    "connect-multiparty": "^2.2.0",
    "cookie-parser": "^1.4.7",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^7.1.2",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-webpack-plugin": "^5.0.2",
    "express": "^5.1.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jasmine-ajax": "^4.0.0",
    "jasmine-core": "^5.10.0",
    "jest": "^29.5.0",
    "jest-config": "^29.5.0",
    "jest-environment-jsdom": "^30.1.2",
    "lint-staged": "^15.4.3",
    "lodash.camelcase": "^4.3.0",
    "nprogress": "^0.2.0",
    "prettier": "^3.5.2",
    "prompt": "^1.0.0",
    "qs": "^6.14.0",
    "replace-in-file": "^3.4.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.24.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "semantic-release": "^15.9.16",
    "shelljs": "^0.8.3",
    "style-loader": "^4.0.0",
    "travis-deploy-once": "^5.0.9",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.0",
    "tslib": "^2.8.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.2.0",
    "webpack": "^5.101.3",
    "webpack-dev-middleware": "^7.4.3",
    "webpack-hot-middleware": "^2.26.1"
  }
}